#%RAML 1.0
title: api-development HRMatches API
version: v0.1
baseUri: http://api-development.hrmatches.com
traits:
  token_required:
    queryParameters:
      token:
        type: string
        description: The server-generated token issued to the user after a successful login
        example: E27B40D6-6C62-6D46-AC144BDF0E9A1D68
        required: true
        minLength: 35
        maxLength: 35
  token_optional:
    queryParameters:
      token:
        type: string
        description: The server-generated token issued to the user after a successful login
        example: E27B40D6-6C62-6D46-AC144BDF0E9A1D68
        required: false
        minLength: 35
        maxLength: 35
  response_delete_ok:
      responses:
        200:
          body:
            application/json:
              example: |
                {
                "message": "DELETE_OK"
                }
  response_post_ok:
    responses:
      200:
        body:
          application/json:
            example: |
              {
              "message": "POST_OK"
              }
  response_put_ok:
      responses:
        200:
          body:
            application/json:
              example: |
                {
                "message": "PUT_OK"
                }

  response_delete_nok:
      responses:
        3xx:
        4xx:
        5xx:
          body:
            application/json:
              example: |
                {
                "message": "DELETE_NOK"
                }
  response_get_nok:
    responses:
      3xx:
      4xx:
      5xx:
        body:
          application/json:
            example: |
              {
              "message": "GET_NOK"
              }
  response_post_nok:
      responses:
        3xx:
        4xx:
        5xx:
          body:
            application/json:
              example: |
                {
                "message": "POST_NOK"
                }
  response_put_nok:
      responses:
        3xx:
        4xx:
        5xx:
          body:
            application/json:
              example: |
                {
                "message": "PUT_NOK"
                }


#################
# API Endpoints
#################

# validate_token:

/validate_token:
  /{token}:
    get:
      #description: validate a users token generated by server after succesful authentication
      responses:
        200:
          body:
            application/json:
              type: object
              properties:
                tokenIsValid: boolean
              example:
                value: {
                    "tokenIsValid": true
                  }
        3xx:
        4xx:
        5xx:
          body:
            application/json:
              type: object
              properties:
                tokenIsValid: boolean
              example: |
                {
                    "tokenIsValid": false
                }

# actionItem

/actionItem:
  is: [ token_optional ]
  get:
    is: [response_get_nok]
    responses:
      200:
        body:
          application/json:
            description : an unordered lists of actionitems
            type: array
            items:
              type: object
              properties:
                itemId:
                  type: string
                  description: the item id
                itemLabel:
                  type: string
                  description: the item human readable label
                actionUrl:
                  type: string
                  description: the action (a http request) to execute when chosen
                actionItemIconName:
                  type: string
                  description: name of actionitem
            example:
              [{
                "itemId": "0DDCBBD4-6C62-6D46-AC1475645F5FUG4D",
                "itemLabel": "Dashboard",
                "actionUrl": "/dashboard",
                "actionItemIconName": "dashboard_icon"
              },{
                "itemId": "0DDCB5D4-6C62-6D46-AC1475645F55UG4D",
                "itemLabel": "Kandidaten",
                "actionUrl": "/candidates",
                "actionItemIconName": "candidates_icon"
              },{
                "itemId": "0DDCBBD4-6C62-6R46-AC1475645F55UG4D",
                "itemLabel": "Vacatures",
                "actionUrl": "/jobs",
                "actionItemIconName": "jobs_icon"
              },{
                "itemId": "0DDCBBD4-6C62-6D46-AC1465645F55UG4D",
                "itemLabel": "Statistieken",
                "actionUrl": "/statistics",
                "actionItemIconName": "statistics_icon"
              },{
                "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                "itemLabel": "Actief",
                "actionUrl": "/candidates/mostActive",
                "actionItemIconName": "dashboard_icon"
              },{
                "itemId": "0DDCBBD4-6C62-6D46-AC1575645F74FG4D",
                "itemLabel": "Recent",
                "actionUrl": "/candidates/recent",
                "actionItemIconName": "recentCandidates_icon"
              }]
  delete:
    is: [response_delete_ok,response_delete_nok]
    description: delete the specified actionMenuItem by id
    queryParameters:
      body:
        type: string
        required: true
  post:
    is: [response_post_ok,response_post_nok]
    description: create a new actionMenuItem
    body:
      application/json:
        type: object
        properties:
          itemId:
            type: string
            description: the item id
          itemLabel:
            type: string
            description: the item human readable label
          actionUrl:
            type: string
            description: the action (a http request) to execute when chosen
          actionItemIconName:
            type: string
            description: name of actionitem
        example: |
          {
            "itemId": "0DDCBBD4-6C62-6D46-AC1575645F74FG4D",
            "itemLabel": "Recent",
            "actionUrl": "/candidates/recent",
            "actionItemIconName": "recentCandidates_icon"
          }
  put:
    is: [response_put_ok,response_put_nok]
    description: edit the resource
    body:
      application/json:
        type: object
        properties:
          itemId:
            type: string
            description: the item id
          itemLabel:
            type: string
            description: the item human readable label
          actionUrl:
            type: string
            description: the action (a http request) to execute when chosen
          actionItemIconName:
            type: string
            description: name of actionitem
        example:
          {
            "itemId": "0DDCBBD4-6C62-6D46-AC1575645F74FG4D",
            "itemLabel": "Recent",
            "actionUrl": "/candidates/recent",
            "actionItemIconName": "recentCandidates_icon"
          }

# menu

/menu:
  is: [ token_optional,response_get_nok ]
  get:
    description: retrieve menus for application
    queryParameters:
      depth:
        type: number
        default: 0
        description: how many levels should the menu at maximum contain? 0 = no submenus, 1 = max 1 submenu etc
        example: 2
        required: false
      node:
        type: string
        default: ""
        description: What is the menu's parent?
        example: "0DDCBBD4-6C62-6D46-AC1475645F74FG4D"
        required: true
    responses:
      200:
        body:
          application/json:
            type: array
            items:
              type: object
              properties:
                itemId:
                  type: string
                itemLabel:
                  type: string
                parent:
                  type: string
                orderBy:
                  description: |
                    possible values are:
                    'natural' [type: string] - same order as in db table,
                    'alphabetic' [type: string] - in ascending order
                    'custom' [type: integer]- a user provided integer value)
                  type: string
                sortOrder:
                  description: the user-defined sort order of a menu item
                  type: integer
                  required: false
                type:
                  description: |
                    a menu item can be single, separator
                    (a line separating singleitems) or
                    compound (containing a submenu)
                  type: string
                actionUrl:
                  type: string
                menuItemIconName:
                  type: string
                  description: name of menuitem
            example:
                [{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "Dashboard",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "singleItem",
                  "actionUrl": "/dashboard",
                  "menuItemIconName": "dashboard_icon"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "itemLabel": "Vacatures",
                  "parent":"",
                  "orderBy": "natural",
                  "type":"singleItem",
                  "actionUrl": "/jobs",
                  "menuItemIconName": "jobs_icon"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "itemSeparator",
                  "actionUrl": "",
                  "menuItemIconName": ""
                }]
  post:
    is: [response_post_ok,response_post_nok]
    body:
      application/json:
        type: object
        properties:
          itemId:
            type: string
          itemLabel:
            type: string
          parent:
            type: string
          orderBy:
            description: |
              possible values are:
              'natural' [type: string] - same order as in db table,
              'alphabetic' [type: string] - in ascending order
              'custom' [type: integer]- a user provided integer value)
            type: string
          sortOrder:
            description: the user-defined sort order of a menu item
            type: integer
            required: false
          type:
            description: |
              a menu item can be single, separator
              (a line separating singleitems) or
              compound (containing a submenu)
            type: string
          actionUrl:
            type: string
          menuItemIconName:
            type: string
            description: name of menuitem
        example:
          {
            "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
            "itemLabel": "",
            "parent": "",
            "orderBy": "natural",
            "type": "itemSeparator",
            "actionUrl": "",
            "menuItemIconName": ""
          }
  put:
    is: [response_put_ok,response_put_nok]
    body:
      application/json:
        type: object
        properties:
          itemId:
            type: string
          itemLabel:
            type: string
          parent:
            type: string
          orderBy:
            description: |
              possible values are:
              'natural' [type: string] - same order as in db table,
              'alphabetic' [type: string] - in ascending order
              'custom' [type: integer]- a user provided integer value)
            type: string
          sortOrder:
            description: the user-defined sort order of a menu item
            type: integer
            required: false
          type:
            description: |
              a menu item can be single, separator
              (a line separating singleitems) or
              compound (containing a submenu)
            type: string
          actionUrl:
            type: string
          menuItemIconName:
            type: string
            description: name of menuitem
        example:
          {
            "itemId": "",
            "itemLabel": "",
            "parent": "",
            "orderBy": "natural",
            "type": "itemSeparator",
            "actionUrl": "",
            "menuItemIconName": ""
          }

  delete:
    is: [response_delete_ok,response_delete_nok]
    queryParameters:
      id:
        type: string
        required: true
  /{node}:
    description: Where in the menu context tree does this menu appear? value "" means there is no parent menu, any other value implies this is a submenu.
    get:
      responses:
        200:
          body:
            Application/json:
              type: object
              properties:
                itemId:
                  type: string
                itemLabel:
                  type: string
                parent:
                  type: string
                orderBy:
                  type: string
                type:
                  type: string
                actionUrl:
                  type: string
                menuItemIconName:
                  type: string
                  description: name of menuitem
              example:
                {
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "Dashboard",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "singleItem",
                  "actionUrl": "/dashboard",
                  "menuItemIconName": "dashboard_icon"
                }

#validate_secretkey

/validate_secretkey:
  is: [response_post_ok]
  post:
    body:
      application/json:

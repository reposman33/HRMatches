# translation

/translation:
  is: [ token_optional,
    response_get_nok ] # this covers all responses except GET 200
  get:
    queryParameters:
      language:
        required: false
      languageKey:
        required: false
    responses:
      200:
        body:
          application/json:
            type: object
            facets:
              configuration:
                properties:
                  columns:
                    type: object
                    properties:
                      visible: boolean
                      columnName: boolean
                      header_visible: boolean
                      header_text: boolean
                      cell_editable: boolean
                  row_editable:
                      type: boolean
                  pagination:
                    type: object
                    facets:
                      enable: boolean
                      itemsPerPage: integer
                      maxSize: integer
              data:
                type: array
                facets:
                  objects:
                    properties:
                      id: string
                examples:
                  array: |
                    {
                    "configuration": {
                    "columns": [
                        {
                           "visible": true,
                           "columnName": "id",
                           "header_visible": true,
                           "header_text": "id",
                           "cell_editable": false
                        },
                        {
                           "visible": true,
                           "columnName": "DisplayName",
                           "header_visible": true,
                           "header_text": "DisplayName",
                           "cell_editable": false
                        },
                        {
                           "visible": true,
                           "columnName": "displayName",
                           "header_visible": true,
                           "header_text": "displayName",
                           "cell_editable": false
                        }
                     ],
                      "row_editable": true,
                      "pagination": {
                          "enable": true,
                          "itemsPerPage": 15,
                          "maxSize": 10
                        }
                      },
                  "data": [
                     {
                        "DisplayName": "Over kandidaat",
                        "id": "APPLICANT_ABOUT_CANDIDATE"
                     },
                     {
                        "DisplayName": "Activiteiten",
                        "id": "APPLICANT_ACTIVITY"
                     },
                     {
                        "DisplayName": "Notitie",
                        "id": "APPLICANT_ADD_NOTE"
                     },
                     {
                        "DisplayName": "Taak",
                        "id": "APPLICANT_ADD_TASK"
                     }]
# trackdata

/trackdata:
  post:
    body:
      application/json:
        properties:
          trackingData:
            required: true
            type: object
            description: A collection of browser object properties to identify the client
            properties:
              token: string
              state: string
              protocol: string
              hostname: string
              href: string
              appVersion: string
              language: string
              platform: string
              userAgent: string
              screenSize: string
              colorDepth: string
        example: |
          {
            "trackingData": {
              "token": "0DDCBBD4-6C62-6D46-AC1475645F74A49D",
              "state": "settings.account.person",
              "protocol": "http:",
              "hostname": "ojt.hrmatches.com",
              "href": "http://ojt.hrmatches.com/#/settings/account",
              "appVersion": "5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/51.0.2704.84 Safari/537.36",
              "language": "en-US",

/account:
  is: [ token_required,
    response_delete_nok,
    response_get_nok,
    response_put_nok,
    response_delete_ok,
    response_put_ok ] # this covers all responses except GET 200
  get:
    queryParameters:
      personId:
        type: string
        required: true
    responses:
      200:
        body:
          application/json:
            type: object
            properties:
              firstName: string
              phoneNumber: string
              infix: string
              language: string
              id: string
              emailaddress: string
              lastName: string
            example:
              {
               "firstName": "demo",
               "phoneNumber": "",
               "infix": "",
               "language": "nl_NL",
               "id": "7142d9e-b1be-41ac-bbcf-a6a1f4d31170",
               "emailaddress": "demo@hrmatches.com",
               "lastName": "HRMatches"
            }
  delete:
    queryParameters:
      personId:
        required: true
        type: string
  put:
    body:
      application/json:
        type: object
        properties:
          person:
            required: true
            type: object
            properties:
              firstName: string
              phoneNumber: string
              infix: string
              language: string
              id: string
              emailaddress: string
              lastName: string
          token: string
        example: |
          {
            "person": {
              "firstName": "demo",
              "phoneNumber": "",
              "infix": "",
              "language": "nl_NL",
              "id": "7142d9e-b1be-41ac-bbcf-a6a1f4d31170",
              "emailaddress": "demo@hrmatches.com",
              "lastName": "HRMatches"
            },
            "token": "E4CB9DB5-6C62-6D46-AC1443222464C278"
          }

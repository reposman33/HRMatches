#%RAML 1.0
title: HRMatches API
version: v0.1
baseUri: http://api-development.hrmatches.com
traits:
 token_required:
    queryParameters:
      token:
        displayName: token
        type: string
        description: The server-generated token issued to the user after a successful login
        example: E27B40D6-6C62-6D46-AC144BDF0E9A1D68
        required: true
        minLength: 35
        maxLength: 35
 token_optional:
    queryParameters:
      token:
        displayName: token
        type: string
        description: The server-generated token issued to the user after a successful login
        example: E27B40D6-6C62-6D46-AC144BDF0E9A1D68
        required: false
        minLength: 35
        maxLength: 35

#################
# API Endpoints
#################

/validate_token:
  get:
    #description: validate a users token generated by server after succesful authentication
    is: [ token_optional ]
    responses:
      200:
        body:
          application/json:
            type: object
            properties:
              tokenIsValid: boolean
            example:
              value: {
                  "tokenIsValid": true
                }
/actionItem:
  is: [ token_optional ]
  get:
    responses:
      200:
        body:
          application/json:
            description : an unordered lists of actionitems
            type: array
            facets:
              object:
                properties:
                  itemId:
                    displayName: itemId
                    type: string
                    description: the item id
                  itemLabel:
                    displayName: itemLabel
                    type: string
                    description: the item human readable label
                  actionUrl:
                    displayName: actionUrl
                    type: string
                    description: the action (a http request) to execute when chosen
            example:
              value: [
              {
                "itemId": "0DDCBBD4-6C62-6D46-AC1475645F5FUG4D",
                "itemLabel": "Dashboard",
                "actionUrl": "/dashboard"
              },{
                "itemId": "0DDCB5D4-6C62-6D46-AC1475645F55UG4D",
                "itemLabel": "Kandidaten",
                "actionUrl": "/candidates"
              },{
                "itemId": "0DDCBBD4-6C62-6R46-AC1475645F55UG4D",
                "itemLabel": "Vacatures",
                "actionUrl": "/jobs"
              },{
                "itemId": "0DDCBBD4-6C62-6D46-AC1465645F55UG4D",
                "itemLabel": "Statistieken",
                "actionUrl": "/statistics"
              },{
                "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                "itemLabel": "Actief",
                "actionUrl": "/candidates/mostActive"
              },{
                "itemId": "0DDCBBD4-6C62-6D46-AC1575645F74FG4D",
                "itemLabel": "Recent",
                "actionUrl": "/candidates/recent"
              }]
/menu:
  is: [ token_optional ]
  get:
    description: retrieve menus for application
    queryParameters:
      depth:
        displayName: depth
        type: number
        default: 0
        description: how many levels should the menu at maximum contain? 0 = no submenus, 1 = max 1 submenu etc
        example: 2
        required: false
      node:
        displayName: Node
        type: string
        default: ""
        description: What is the menu's parent?
        example: "0DDCBBD4-6C62-6D46-AC1475645F74FG4D"
        required: true
    responses:
      200:
        body:
          application/json:
            type: array
            facets:
              object:
                properties:
                  itemId:
                    displayName: itemId
                    type: string
                  itemLabel:
                    displayName: itemLabel
                    type: string
                  parent:
                    displayName: parent
                    type: string
                  orderBy:
                    displayName: orderBy
                    type: string
                  type:
                    displayName: type
                    type: string
                  actionUrl:
                    displayName: actionUrl
                    type: string
            examples:
              singleLevel:
                [{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "Dashboard",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "singleItem",
                  "actionUrl": "/dashboard"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "itemLabel": "Vacatures",
                  "parent":"",
                  "orderBy": "natural",
                  "type":"singleItem",
                  "actionUrl": "/jobs"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "itemSeparator",
                  "actionUrl": ""
                }]
              multilevel:
                [{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "Dashboard",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "singleItem",
                  "actionUrl": "/dashboard"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "itemLabel": "Vacatures",
                  "parent":"",
                  "orderBy": "natural",
                  "type":"singleItem",
                  "actionUrl": "/jobs"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FG4D",
                  "itemLabel": "",
                  "parent": "",
                  "orderBy": "natural",
                  "type":itemSeparator,
                  "actionUrl": ""
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "itemLabel": "Kandidaten",
                  "parent": "",
                  "orderBy": "natural",
                  "type":"compoundItem",
                  "actionUrl": "/candidates"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "itemLabel": "Actief",
                  "parent": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "orderBy": "natural",
                  "type":"singleItem",
                  "actionUrl": "/candidates/mostActive"
                },{
                  "itemId": "0DDCBBD4-6C62-6D46-AC1575645F74FG4D",
                  "itemLabel": "Recent",
                  "parent": "0DDCBBD4-6C62-6D46-AC1475645F74FF4D",
                  "orderBy": "natural",
                  "type":"singleItem",
                  "actionUrl": "/candidates/recent"
                },{
                  "itemId": "0DDCBBD4-6C61-6D46-AC1475645F74FG4D",
                  "itemLabel": "Statistieken",
                  "parent": "",
                  "orderBy": "natural",
                  "type": "singleItem",
                  "actionUrl": "/statistics"
                }]
  /{node}:
    description: Where in the menu context tree does this menu appear? value "" means there is no parent menu, any other value implies this is a submenu.
    get:
      responses:
        200:
          body:
            Application/json:
              properties:
                node:
                  displayName: Parent node
                  type: string
                  default: ""
                  description: Where in the menu context tree does this menu appear? value "" means there is no parent menu, any other value implies this is a submenu.
                  example: "0DDCBBD4-6C62-6D46-AC1475645F74FG4D"
                  required: true
